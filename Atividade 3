using System;
 namespace atividade
 {
    class atividade03
    {
        public static void Main(string[] args)
        {
            Console.WriteLine("Coloque um número:");
            int numero = Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("Seu numero é:" + numero);
        }
    }
 }
